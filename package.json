{
  "name": "qwik-conference",
  "description": "Blank Qwik starter app. Vite.js tooling. Express.js server. Eslint code linting. Prettier code formatter. Tailwind CSS framework.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.express.tsx",
    "dev": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"**/*.ts*\"",
    "serve": "node --inspect server/entry.express.js",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.28.0",
    "eslint-plugin-qwik": "^0.16.2",
    "node-fetch": "3.3.0",
    "prettier": "2.7.1",
    "typescript": "4.9.3",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "dependencies": {
    "@builder.io/qwik": "^0.20.1",
    "@builder.io/qwik-city": "^0.5.2",
    "undici": "^5.16.0"
  }
}
